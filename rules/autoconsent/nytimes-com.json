{
    "name": "nytimes",
    "runContext": {
        "urlPattern": "^https://(www\\.)?nytimes.com"
    },
    "cosmetic": true,
    "prehideSelectors": ["#dock-container[data-testid='gdprDock']"],
    "detectCmp": [
        {
            "exists": "#dock-container[data-testid='gdprDock']"
        }
    ],
    "detectPopup": [{ "visible": "#dock-container[data-testid=\"gdprDock\"]" }],
    "optIn": [
        {
            "click": "#dock-container[data-testid=\"gdprDock\"] button:nth-child(0)"
        }
    ],
    "optOut": [
        {
            "click": "#dock-container[data-testid=\"gdprDock\"] button:nth-child(1)"
        }
    ]
}